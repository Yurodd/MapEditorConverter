     closest_secondpoint2 = FirstOf(SortedArray(FilteredArray(secondpoint2,IsInLineOfSight(ArrayElement(),EyePosition())),DotProduct(FacingDirectionOf(),DirectionTowards(ArrayElement(),EyePosition()))));
    }

     AllVertices = [
